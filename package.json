{
  "name": "expressjsapp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "watchify": "watchify -vd -e src/main.js -o public/dist/build.js",
    "build": "cross-env NODE_ENV=production browserify -g envify -p [ vueify/plugins/extract-css -o public/dist/build.css ] -e src/main.js | uglifyjs -c warnings=false -m > public/dist/build.js"
  },
  "browserify": {
    "transform": [
      "babelify",
      "vueify"
    ]
  },
  "browser": {
    "vue": "vue/dist/vue.common.js"
  },
  "dependencies": {
    "body-parser": "~1.16.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.0",
    "express": "~4.14.1",
    "jade": "~1.11.0",
    "morgan": "~1.7.0",
    "serve-favicon": "~2.3.2",
    "vue": "^2.2.6",
    "vue-router": "^2.5.2"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "connect-history-api-fallback": "^1.3.0",
    "cross-env": "^4.0.0",
    "envify": "^4.0.0",
    "nodemon": "^1.11.0",
    "uglifyjs": "^2.4.10",
    "vueify": "^9.4.1",
    "watchify": "^3.9.0"
  }
}
